<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.happyhouse5.dao.HouseInfoDao">
  <select id="selectHouseList" parameterType="string" resultType="HouseInfo">
    select *
    from houseinfo
    where dongcode = #{dongCode}
  </select>
</mapper>